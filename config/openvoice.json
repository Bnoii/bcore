{
  "default_engine": "openvoice",
  "engines": {
    "openvoice": {
      "base_url": "http://openvoice-bridge:5005"
    },
    "elevenlabs": {
      "api_key_env": "ELEVENLABS_API_KEY"
    },
    "huggingface": {
      "token_env": "HUGGINGFACE_API_TOKEN",
      "model": "facebook/mms-tts-eng"
    }
  },
  "storage": {
    "save_uploads": true,
    "upload_dir": "./uploads/voices"
  }
}
